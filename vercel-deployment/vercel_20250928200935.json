{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/chat.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/chat",
      "dest": "/api/chat"
    }
  ],
  "env": {
    "DEEPSEEK_API_KEY": "@deepseek-api-key"
  }
}
