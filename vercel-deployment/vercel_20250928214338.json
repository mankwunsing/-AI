{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "mobile.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/chat.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/mobile.html",
      "dest": "/mobile.html"
    },
    {
      "src": "/api/chat",
      "dest": "/api/chat"
    }
  ]
}
